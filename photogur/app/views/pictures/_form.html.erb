<%= form_for(@picture) do |f| %>

<p>
<%= f.label :title %><br/>
<%= f.text_field :title %>
</p> 

<p>
<%= f.label :artist %><br/>
<%= f.text_field :artist %>
</p>

<p>
<%= f.label :image %><br/>
<%= f.text_field :image, size: 20,
				placeholder: "JPG, GIF, or PNG File Name"%>
</p>

<p>
<%= f.submit %> |
<%= link_to "Cancel", pictures_path %>
</p>

<%end%>