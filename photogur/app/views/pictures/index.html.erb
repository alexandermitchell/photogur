<p class='link'><%= link_to "Add New Picture", new_picture_path %></p>
<% @pictures.each do |picture| %>

<div class='wrapper' style='background-image: url("<%= image_path picture.image %>")'>

	<%#= image_tag picture.image %>
	<%= link_to picture do %>
	<div class='text'>

		<div class='header'><%= picture.title %></div>
		<div class='subtitle'><%= picture.artist %></div>
	</div>
	<%end%>
</div>

<%end%>

